#!/bin/sh
if [ -e $WILMA_HOME/lib/wilma.jar ]
then
  cd ${WILMA_HOME}/lib
  if [ x$OS = xWindows_NT ]
  then
    java -cp xalan.jar\;crimson.jar\;jaxp.jar\;wilma.jar org.wilmascope.control.WilmaMain
  else
    java -cp /usr/share/java/postgresql.jar:xalan.jar:crimson.jar:jaxp.jar:wilma.jar -Djdbc.drivers=org.postgresql.Driver org.wilmascope.control.WilmaMain
  fi
else
  echo You must set the WILMA_HOME environment variable
fi
#java -cp xalan.jar\;crimson.jar\;jaxp.jar\;.\;classes wilmatoo.control.Test
